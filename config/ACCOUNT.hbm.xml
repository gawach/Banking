<?xml version="1.0" ?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN""http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>    <class name="com.oreilly.struts.banking.entity.account.Account" table="ACCOUNT">        <id name="id" column="ID"  type="int" >            <generator class="assigned" />        </id>        <property name="accessNumber" type="int" column="ACCESS_NUMBER" />        <property name="pinNumber" type="int" column="PIN_NUMBER" />        <property name="balance" type="int" column="BALANCE" />        <property name="memberId" type="int" column="MEMBER_ID" />    </class></hibernate-mapping>