<?xml version="1.0" ?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN""http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>    <class name="com.oreilly.struts.banking.entity.member.Member" table="MEMBER">        <id name="id" column="ID"  type="int" >            <generator class="assigned" />        </id>        <property name="name" type="string" column="NAME" />        <property name="email" type="string" column="EMAIL" />        <property name="password" type="string" column="PASSWORD" />        <property name="category" type="string" column="CATEGORY" />    </class></hibernate-mapping>